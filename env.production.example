# Production Environment Variables for Railway
# Copy these to Railway environment variables

# Rails Configuration
RAILS_ENV=production
RAILS_MASTER_KEY=your_rails_master_key_here
RAILS_SERVE_STATIC_FILES=true
RAILS_LOG_TO_STDOUT=true
RAILS_MAX_THREADS=5

# Database Configuration (Railway will provide this)
DATABASE_URL=postgresql://username:password@host:port/database

# Redis Configuration (Your Railway Redis instance)
REDIS_URL=redis://ballast.proxy.rlwy.net:31355

# Application Configuration
PORT=3000
